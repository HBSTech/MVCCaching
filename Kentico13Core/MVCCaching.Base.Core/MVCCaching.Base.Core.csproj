<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>MVCCaching (Base)</Title>
    <Version>4.0.1</Version>
    <PackageId>MVCCaching.Base.Core</PackageId>
    <Authors>Trevor Fayas</Authors>
    <Company>Heartland Business Systems</Company>
    <PackageProjectUrl>https://github.com/KenticoDevTrev/MVCCaching</PackageProjectUrl>
    <Description>Base Attribute and Intefaces for MVC Caching to be implemented with.    Designed for XperienceCommunity.MVCCaching.Core code files found on github.com, but could be extended to other uses.</Description>
    <Copyright>2022 Heartland Business Systems</Copyright>
    <PackageIcon>hbs-favicon-96x96.png</PackageIcon>
    <RepositoryUrl>https://github.com/KenticoDevTrev/MVCCaching</RepositoryUrl>
    <PackageReleaseNotes>Added modifier to the ToCacheRepositoryContextNameIdentifier() to allow 'disabling' cache during editing on preview mode.</PackageReleaseNotes>
    <PackageTags>Xperience, Kentico, MVCCaching</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\hbs-favicon-96x96.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
